{
    "name": "",
    "steps": [
        {
            "name": "columntransformer",
            "transformers": [
                {
                    "transformer": {
                        "name": "pipeline-1",
                        "steps": [
                            {
                                "name": "simpleimputer",
                                "params": {
                                    "add_indicator": false,
                                    "copy": true,
                                    "fill_value": null,
                                    "missing_values": null,
                                    "strategy": "most_frequent",
                                    "verbose": 0
                                }
                            },
                            {
                                "name": "onehotencoder",
                                "params": {
                                    "categories": "auto",
                                    "drop": null,
                                    "handle_unknown": "ignore",
                                    "sparse": false
                                }
                            }
                        ]
                    },
                    "column_selector": "category"
                },
                {
                    "transformer": {
                        "name": "pipeline-2",
                        "steps": [
                            {
                                "name": "simpleimputer",
                                "params": {
                                    "add_indicator": false,
                                    "copy": true,
                                    "fill_value": null,
                                    "missing_values": null,
                                    "strategy": "mean",
                                    "verbose": 0
                                }
                            },
                            {
                                "name": "standardscaler",
                                "params": {
                                    "copy": true,
                                    "with_mean": true,
                                    "with_std": true
                                }
                            }
                        ]
                    },
                    "column_selector": "number"
                }
