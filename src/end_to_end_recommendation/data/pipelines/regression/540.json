{
    "name": "",
    "steps": [
        {
            "name": "columntransformer",
            "transformers": [
                {
                    "transformer": {
                        "name": "pipeline",
                        "steps": [
                            {
                                "name": "simpleimputer",
                                "params": {
                                    "add_indicator": false,
                                    "copy": true,
                                    "fill_value": null,
                                    "missing_values": null,
                                    "strategy": "mean",
                                    "verbose": 0
                                }
                            },
                            {
                                "name": "featureunion",
                                "transformers": [
                                    {
                                        "name": "pipeline-1",
                                        "steps": [
                                            {
                                                "name": "minmaxscaler",
                                                "params": {
                                                    "copy": true,
                                                    "feature_range": [
                                                        0,
                                                        1
                                                    ]
                                                }
                                            },
                                            {
                                                "name": "rbfsampler",
                                                "params": {
                                                    "gamma": 1.0,
                                                    "n_components": 100,
                                                    "random_state": null
                                                }
                                            }
                                        ]
                                    },
                                    {
                                        "name": "pipeline-2",
                                        "steps": [
                                            {
                                                "name": "standardscaler",
                                                "params": {
                                                    "copy": true,
                                                    "with_mean": true,
                                                    "with_std": true
                                                }
                                            },
                                            {
                                                "name": "truncatedsvd",
                                                "params": {
                                                    "algorithm": "randomized",
                                                    "n_components": 2,
                                                    "n_iter": 5,
                                                    "random_state": null,
                                                    "tol": 0.0
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    "column_selector": "number"
                }
            ],
            "remainder": "passthrough"
        },
        {
            "name": "ridge",
            "params": {
                "alpha": 1.0,
                "copy_X": true,
                "fit_intercept": true,
                "max_iter": null,
                "normalize": false,
                "random_state": null,
                "solver": "auto",
                "tol": 0.001
            }
        }
    ]
}